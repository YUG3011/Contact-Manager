generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Contact {
  id      String  @id @map("_id") @default(auto()) @db.ObjectId
  name    String
  email   String
  phone   String?
  notes   String?
  createdAt DateTime @default(now())
}
